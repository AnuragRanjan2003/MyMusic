<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mymusic">

    <application
        android:allowBackup="true"
        android:icon="@drawable/music"
        android:label="@string/app_name"
        android:roundIcon="@drawable/music"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyMusic">
        <activity
            android:name=".songlistActivity"
            android:exported="true"
            android:parentActivityName=".songActivity2"/>
        <activity
            android:name=".loginActivity"
            android:exported="true"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".signupActivity"
            android:exported="true"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true" />
        <activity
            android:name=".songActivity2"
            android:exported="true"
            android:parentActivityName=".songlistActivity" />
    </application>

</manifest>